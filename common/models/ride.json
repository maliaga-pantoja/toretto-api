{
  "name": "ride",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "address": {
      "type": "string"
    },
    "customers": {
      "type": "array"
    },
    "district": {
      "type": "string"
    },
    "comments": {
      "type": [
        "string"
      ]
    },
    "started_at": {
      "type": "date"
    },
    "ended_at": {
      "type": "date"
    },
    "status": {
      "type": "number",
      "default": 0
    },
    "gps": {
      "type": "string"
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "travel": {
      "type": "belongsTo",
      "model": "travel",
      "foreignKey": ""
    },
    "member": {
      "type": "belongsTo",
      "model": "member",
      "foreignKey": ""
    },
    "company": {
      "type": "belongsTo",
      "model": "company",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
