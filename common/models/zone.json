{
  "name": "zone",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },

  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "nhc": {
      "type": "string"
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "company": {
      "type": "belongsTo",
      "model": "company",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
