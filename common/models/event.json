{
  "name": "event",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "description": {
      "type": "string"
    },
    "location": {
      "type": "geopoint"
    },
    "title": {
      "type": "string"
    },
    "created_at": {
      "type":"date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "ride": {
      "type": "belongsTo",
      "model": "ride",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
